{
  "expo": {
    "name": "gradproj",
    "slug": "gradproj",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./app/assets/finallogo.png",
    "splash": {
      "image": "./app/assets/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "favicon": "./app/assets/favicon.png"
    },
    "android":{
      "permissions":["expo-barcode-scanner expo-camera"," expo-camera","expo-image-picker","expo-media-library","expo-image-picker","expo-media-library"],
      "package":"com.Health.archive",
      "versionCode": 1
    }
  }
}
